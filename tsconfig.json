{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "allowJs": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5"
  },
  "include": ["client", "index.d.ts"],
  "exclude": ["node_modules", "**/*.spec.ts", "**/*.test.ts"]
}
